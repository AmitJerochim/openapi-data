          }
        }, {
          "in" : "query",
          "name" : "rentalEnd",
          "required" : true,
          "description" : "Planned rental end",
          "schema" : {
            "type" : "string",
            "format" : "date-time",
            "example" : "2019-10-23T07:00:00+02:00"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Request successful",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 200
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
