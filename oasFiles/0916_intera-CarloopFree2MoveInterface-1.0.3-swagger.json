    "/online-rental-groups" : {
      "get" : {
        "description" : "Retrieves available rental groups in a given timerange for given stations",
        "parameters" : [ {
          "in" : "query",
          "name" : "latitude",
          "description" : "Latitude of search position",
          "required" : true,
          "schema" : {
            "type" : "number",
            "example" : 50.8231631
          }
        }, {
          "in" : "query",
          "name" : "longitude",
          "description" : "Longitude of search position",
          "required" : true,
          "schema" : {
            "type" : "number",
            "example" : 6.2481247
          }
        }, {
          "in" : "query",
          "name" : "radius",
          "description" : "Search radius around search position in kilometers",
          "required" : true,
          "schema" : {
            "minimum" : 1,
            "maximum" : 50,
            "type" : "integer",
            "example" : 10
          }
        }, {
          "in" : "query",
          "name" : "vehicleGroup",
          "description" : "Vehicle group filter (car, van, bus)",
          "required" : true,
          "schema" : {
            "type" : "string",
            "enum" : [ "van", "car", "bus" ],
            "example" : "car"
          }
        }, {
          "in" : "query",
          "name" : "vehicleAttributes",
          "description" : "Vehicle attribute requirements",
          "schema" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/VehicleAttributes"
            }
          }
        }, {
          "in" : "query",
          "name" : "rentalStart",
          "required" : true,
          "description" : "Planned rental start",
          "schema" : {
            "type" : "string",
            "format" : "date-time",
            "example" : "2019-10-21T08:00:00+02:00"
          }
        }, {
          "in" : "query",
          "name" : "rentalEnd",
          "required" : true,
          "description" : "Planned rental end",
          "schema" : {
            "type" : "string",
            "format" : "date-time",
            "example" : "2019-10-23T07:00:00+02:00"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Request successfull.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 200
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : "Your search was successful."
                      }
                    },
                    "results" : {
                      "type" : "array",
                      "description" : "Available online rental groups - grouped by firm",
                      "items" : {
                        "type" : "object",
                        "properties" : {
                          "firmId" : {
                            "type" : "integer",
                            "description" : "ID einer Firm",
                            "example" : 1
                          },
                          "name" : {
                            "type" : "string",
                            "description" : "Name/Description of dealer location",
                            "example" : "Autohaus XYZ Hamburg"
                          },
                          "rentalTerms" : {
                            "type" : "object",
                            "description" : "Rental terms that must be accepted by user before reservation. Includes cancellation terms.",
                            "properties" : {
                              "label" : {
                                "type" : "string",
                                "example" : "AGB"
                              },
                              "url" : {
                                "type" : "string",
                                "example" : "https://intera.de/agb"
                              }
                            }
                          },
                          "address" : {
                            "type" : "object",
                            "required" : [ "countryCode", "place", "postalCode", "address1" ],
                            "properties" : {
                              "countryCode" : {
                                "type" : "string",
                                "description" : "ISO Country Code",
                                "example" : "DE"
                              },
                              "place" : {
                                "type" : "string",
                                "example" : "Norderstedt"
                              },
                              "postalCode" : {
                                "type" : "string",
                                "example" : "22848"
                              },
                              "address1" : {
                                "type" : "string",
                                "description" : "Street and number",
                                "example" : "Aspelohe 27c"
                              },
                              "address2" : {
                                "type" : "string",
                                "description" : "Further address information"
                              }
                            }
                          },
                          "currency" : {
                            "$ref" : "#/components/schemas/Currency"
                          },
                          "vatRate" : {
                            "type" : "number",
                            "description" : "VAT rate, given as factor",
                            "format" : "float",
                            "example" : 1.19
                          },
                          "onlineRentalGroups" : {
                            "type" : "array",
                            "items" : {
                              "$ref" : "#/components/schemas/OnlineRentalGroup"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
