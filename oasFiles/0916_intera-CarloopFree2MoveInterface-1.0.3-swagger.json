                        "type" : "string",
                        "example" : "Search was sucessful."
                      }
                    },
                    "currency" : {
                      "$ref" : "#/components/schemas/Currency"
                    },
                    "vatRate" : {
                      "type" : "number",
                      "description" : "VAT rate, given as factor",
                      "format" : "float",
                      "example" : 1.19
                    },
                    "rentAdditionals" : {
                      "description" : "Available additionals for time range and rental group",
                      "type" : "array",
                      "items" : {
                        "$ref" : "#/components/schemas/RentAdditionals"
                      }
                    },
                    "insurance" : {
                      "description" : "Information on possible insurance options for rental group",
                      "type" : "object",
                      "required" : [ "defaultDeductibleId", "defaultDeductibleAmount" ],
                      "properties" : {
                        "availableDeductibles" : {
                          "description" : "Available liability deductibles (doesnt include default deductible)",
                          "type" : "array",
                          "items" : {
                            "type" : "object",
                            "properties" : {
                              "deductibleId" : {
                                "type" : "integer",
                                "description" : "UID of deductible",
                                "example" : 13
                              },
                              "deductibleAmount" : {
                                "type" : "number",
                                "format" : "float",
                                "description" : "Amount of deductible",
                                "example" : 500.0
                              },
                              "deductiblePrice" : {
                                "type" : "number",
                                "format" : "float",
                                "description" : "Price in timerange for deductible (net)",
                                "example" : 9.99
                              }
                            }
                          }
                        },
                        "defaultDeductibleId" : {
                          "description" : "UID of default deductible (no extra cost!)",
                          "type" : "integer",
                          "example" : -1
                        },
                        "defaultDeductibleAmount" : {
                          "description" : "Amount of default deductible",
                          "type" : "number",
                          "format" : "float",
                          "example" : 500.0
                        }
                      }
                    },
                    "milage" : {
                      "description" : "Milage information for rental group",
                      "type" : "object",
                      "required" : [ "inclusiveMilage", "priceExtraMilage" ],
                      "properties" : {
                        "inclusiveMilage" : {
                          "description" : "Milage included in given time range / price",
                          "type" : "integer",
                          "example" : 200
                        },
                        "priceExtraMilage" : {
                          "description" : "Price for additional milage (net)",
                          "type" : "number",
                          "format" : "float",
                          "example" : 0.15
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/vehicle-rentals" : {
      "post" : {
        "description" : "Submit a reservation",
        "requestBody" : {
          "required" : true,
          "content" : {
            "application/json" : {
              "schema" : {
                "allOf" : [ {
                  "type" : "object",
                  "required" : [ "onlineRentalGroupId", "rentalStart", "rentalEnd", "deductibleId", "firmId", "customer" ],
                  "properties" : {
                    "onlineRentalGroupId" : {
                      "type" : "integer",
                      "description" : "UID of chosen online rental group",
                      "example" : 885
                    },
                    "firmId" : {
                      "type" : "integer",
                      "description" : "UID of dealer site",
                      "example" : 109
                    },
                    "rentalStart" : {
                      "description" : "Rental start",
                      "type" : "string",
                      "format" : "date-time",
                      "example" : "2019-10-21T08:00:00+02:00"
                    },
                    "rentalEnd" : {
                      "description" : "Rental end",
                      "type" : "string",
                      "format" : "date-time",
                      "example" : "2019-10-23T07:00:00+02:00"
                    },
                    "deductibleId" : {
                      "description" : "Chosen liability UID",
                      "type" : "integer",
                      "nullable" : true,
                      "example" : 13
                    },
                    "vehicleAttributes" : {
                      "type" : "array",
                      "description" : "Required vehicle attributes",
                      "example" : [ "hasAutomaticTransmission" ],
                      "items" : {
                        "$ref" : "#/components/schemas/VehicleAttributes"
                      }
                    },
                    "rentAdditionals" : {
                      "type" : "array",
                      "description" : "IDs of requested additionals",
                      "example" : [ 291 ],
                      "items" : {
                        "type" : "integer",
                        "example" : 291
                      }
                    },
                    "internalNotes" : {
                      "type" : "string",
                      "description" : "Hint, given by the user",
                      "example" : "Wunschausstattung: Diesel, Automatik"
                    },
                    "customer" : {
                      "$ref" : "#/components/schemas/Customer"
                    }
                  }
                } ]
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Reservation created successfully.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 200
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : "Reservation was created successfully"
                      }
                    },
                    "vehicleRentalId" : {
                      "type" : "integer",
                      "example" : 164585
                    }
                  }
                }
              }
            }
          },
          "409" : {
            "description" : "Reservierung couldnt be created.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 409
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : [ "Reservierung konnte nicht angelegt werden.", "Es ist leider kein Fahrzeug der gewünschten Klasse mehr verfügbar.", "Bitte führen Sie eine neue Suche durch." ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
