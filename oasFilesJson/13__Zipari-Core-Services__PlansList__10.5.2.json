{"openapi":"3.0.0","info":{"version":"10.5.2","title":"PlansList","description":"Common PlansList API"},"paths":{"/plans/":{"get":{"tags":["plans"],"summary":"Get list of plans based on search criteria","description":"The plans API provides the list of plans available in a market segment, zip code or county code.\n","operationId":"plans_list","parameters":[{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PaginationQueryParameters/10.1#/components/parameters/orderingParam"},{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PaginationQueryParameters/10.1#/components/parameters/pageParam"},{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PaginationQueryParameters/10.1#/components/parameters/pageSizeParam"},{"name":"market_segment","in":"query","description":"Market segment e.g IFP, Medicare to search for available plans the member","required":true,"schema":{"type":"string"}},{"name":"coverage_type","in":"query","description":"coverage type (medical/dental)","required":false,"schema":{"type":"string"}},{"name":"zip_code","in":"query","description":"Zipcode (5 digits)","required":true,"schema":{"type":"string"}},{"name":"county_code","in":"query","description":"County code (5 digit FIPS code)","required":true,"schema":{"type":"string"}},{"name":"is_active","in":"query","description":"Boolean indicating whether the plan is active or not","required":false,"schema":{"type":"boolean"}},{"name":"effective_date","in":"query","description":"Effective on provided date (YYYY-MM-DD)","required":false,"schema":{"type":"string"}},{"name":"termination_date","in":"query","description":"Terminated on provided date (YYYY-MM-DD)","required":false,"schema":{"type":"string"}},{"name":"display_name","in":"query","description":"Human readable plan name","required":false,"schema":{"type":"string"}},{"name":"external_id","in":"query","description":"this is the external plan id to filter on","required":false,"schema":{"type":"string"}},{"name":"category_key","in":"query","description":"this filters on plans having a matching element in the associated PlanCategory category field","required":false,"explode":true,"schema":{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PlanCategoryModel/10.1.1#/components/schemas/PlanCategory/properties/key"}},{"name":"exclude_category_key","in":"query","description":"this filters out plans that have a matching element in the associated PlanCategory category field","required":false,"explode":true,"schema":{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PlanCategoryModel/10.1.1#/components/schemas/PlanCategory/properties/key"}}],"responses":{"200":{"description":"An array of Plan Object","content":{"application/json":{"schema":{"required":["count","results"],"type":"object","properties":{"count":{"type":"integer","description":"Count representing the number of objects returned in the results array\n"},"next":{"type":"integer","description":"query identifier defined by the api developer for getting the next set of records\n"},"previous":{"type":"integer","description":"query identifier defined by the api developer for getting the previous set of records\n"},"results":{"type":"array","items":{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/PlanModel/10.4.2#/components/schemas/Plan"}}}}}}},"400":{"description":"Invalid input","content":{"application/json":{"schema":{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/ErrorModel/10.0.2#/components/schemas/Error"}}}},"403":{"description":"Unauthorized","content":{"application/json":{"schema":{"$ref":"https://api.swaggerhub.com/domains/Zipari-Core-Services/ErrorModel/10.0.2#/components/schemas/Error"}}}}}}}}}