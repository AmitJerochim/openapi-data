{
  "components" : {
    "schemas" : {
      "Article" : {
        "properties" : {
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "location" : {
            "maxLength" : 50,
            "type" : "string"
          },
          "name" : {
            "maxLength" : 100,
            "minLength" : 1,
            "type" : "string"
          },
          "recording_company" : {
            "maxLength" : 100,
            "type" : "string"
          },
          "tracks" : {
            "items" : {
              "format" : "int32",
              "type" : "integer"
            },
            "type" : "array"
          },
          "type" : {
            "enum" : [ "SINGLE", "ALBUM", "EP" ],
            "type" : "string"
          }
        },
        "required" : [ "name" ],
        "type" : "object"
      },
      "Artist" : {
        "properties" : {
          "dob" : {
            "format" : "date",
            "type" : "string"
          },
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "name" : {
            "maxLength" : 100,
            "minLength" : 1,
            "type" : "string"
          },
          "nickname" : {
            "maxLength" : 150,
            "minLength" : 1,
            "type" : "string"
          }
        },
        "required" : [ "name", "nickname" ],
        "type" : "object"
      },
      "ArtistRole" : {
        "properties" : {
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "name" : {
            "maxLength" : 100,
            "minLength" : 1,
            "type" : "string"
          }
        },
        "type" : "object"
      },
      "GroupSchema" : {
        "properties" : {
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "name" : {
            "type" : "string"
          }
        },
        "type" : "object"
      },
      "LoginSchema" : {
        "properties" : {
          "name" : {
            "maxLength" : 64,
            "minLength" : 1,
            "type" : "string"
          },
          "password" : {
            "minLength" : 1,
            "type" : "string"
          }
        },
        "required" : [ "name", "password" ],
        "type" : "object"
      },
      "Playlist" : {
        "properties" : {
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "name" : {
            "maxLength" : 100,
            "minLength" : 1,
            "type" : "string"
          },
          "tracks" : {
            "items" : {
              "$ref" : "#/components/schemas/PlaylistItemInfo"
            },
            "type" : "array"
          }
        },
        "required" : [ "name" ],
        "type" : "object"
      },
      "PlaylistItemInfo" : {
        "properties" : {
          "position" : {
            "format" : "int32",
            "type" : "integer"
          },
          "track_id" : {
            "format" : "int32",
            "type" : "integer"
          }
        },
        "type" : "object"
      },
      "ReactionSchema" : {
        "properties" : {
          "entity_id" : {
            "format" : "int32",
            "type" : "integer"
          }
        },
        "type" : "object"
      },
      "RoleSchema" : {
        "properties" : {
          "id" : {
            "format" : "int32",
            "readOnly" : true,
            "type" : "integer"
          },
          "name" : {
            "type" : "string"
          }
        },
        "type" : "object"
      },
