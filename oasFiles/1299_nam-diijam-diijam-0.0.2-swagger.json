        "properties" : {
          "artist_id" : {
            "format" : "int32",
            "type" : "integer"
          },
          "role_id" : {
            "format" : "int32",
            "type" : "integer"
          },
          "track_id" : {
            "format" : "int32",
            "type" : "integer"
          }
        },
        "type" : "object"
      },
