  "paths" : {
    "/vehicle-rentals/{vehicleRentalId}/checkout" : {
      "put" : {
        "description" : "ATTENTION: This route has to be implemented by Free2Move. This route is called by Carloop and NOT implemented in Carloop.",
        "parameters" : [ {
          "in" : "path",
          "name" : "vehicleRentalId",
          "description" : "Id of the rental",
          "required" : true,
          "schema" : {
            "type" : "integer",
            "example" : 1315425
          }
        } ],
        "requestBody" : {
          "required" : true,
          "content" : {
            "application/json" : {
              "schema" : {
                "properties" : {
                  "milage" : {
                    "type" : "integer",
                    "description" : "Vehicle milage at check out time",
                    "example" : 5012
                  },
                  "fuelPercentage" : {
                    "description" : "Fuel status in percent at check out time",
                    "type" : "integer",
                    "example" : 50
                  },
                  "checkout-time" : {
                    "description" : "Datetime the check out took place",
                    "type" : "string",
                    "format" : "date-time",
                    "example" : "2019-10-23T07:00:00+02:00"
                  }
                }
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Check out transmitted successfully.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 200
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : "Reservation updated successfully"
                      }
                    }
                  }
                }
              }
            }
          },
          "404" : {
            "description" : "Reservation not found",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 404
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : [ "Reservierung konnte nicht gefunden werden." ]
                      }
                    }
                  }
                }
              }
            }
          },
          "409" : {
            "description" : "Reservation couldnt be updated.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "object",
                  "properties" : {
                    "status" : {
                      "type" : "integer",
                      "description" : "HTTP status code",
                      "example" : 409
                    },
                    "statusText" : {
                      "type" : "array",
                      "description" : "User hint for last action (success-, or error-message)",
                      "items" : {
                        "type" : "string",
                        "example" : [ "Reservierung konnte nicht aktualisiert werden." ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
